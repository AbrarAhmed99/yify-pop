<div id="movies">
  <div class="row">
  <% for(var i=0; i < movies.length; i++) { %>
      <div class="col-sm-4">
        <div class="media movie">
          <a class="pull-left" href="#">
            <img class="media-object" src="<%= movies[i].CoverImage %>" alt="<%= movies[i].MovieTitleClean %>">
          </a>
          <div class="media-body">
            <h4 class="media-heading"><%= movies[i].MovieTitleClean %></h4>
            <p><a href="<%= baseURL %>/stream?file=<%= encodeURIComponent(movies[i].TorrentUrl) %>">Watch</a></p>
            <p><a href="<%= movies[i].ImdbLink %>">View on IMDB</a></p>
          </div>
        </div>
      </div>
    <% if (i % 3 === 2) { %>
      </div><div class="row">
    <% } %>
  <% } %>
</div>
